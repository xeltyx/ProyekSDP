            <Label x:Name="lblprod4_Copy" Content="Label" HorizontalAlignment="Left" Margin="797,376,0,0" VerticalAlignment="Top" Width="122" />
            <Image x:Name="imgprod4_Copy" HorizontalAlignment="Left" Height="110" Margin="797,266,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
            <Image x:Name="imgprod3_Copy" HorizontalAlignment="Left" Height="110" Margin="650,266,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
            <Image x:Name="imgprod1_Copy" HorizontalAlignment="Left" Height="110" Margin="357,266,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
            <Image x:Name="imgprod2_Copy" HorizontalAlignment="Left" Height="110" Margin="503,266,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
