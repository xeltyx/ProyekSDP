                <Button Content="  My Profile" Height="59" HorizontalContentAlignment="Left" FontSize="20" Margin="0" Cursor="Hand" BorderBrush="#00707070" Background="#FF9696FA" BorderThickness="0,1" Click="Button_Click_1"/>
                <Button Content="  Cart" Height="59" HorizontalContentAlignment="Left" FontSize="20" Cursor="Hand" BorderBrush="#00707070" Background="#FF9696FA" BorderThickness="0,1"/>
            </StackPanel>
            <Image HorizontalAlignment="Left" Height="49" VerticalAlignment="Top" Width="49" Source="person-icon.png"/>
            <Button x:Name="logoutbtn" Content="  Logout" HorizontalContentAlignment="Left" FontSize="20" Cursor="Hand" BorderBrush="#00707070" Background="#FFE68FA7" BorderThickness="0,1" Margin="0,538,0,0" Click="Button_Click"/>
        </Grid>
        <Image x:Name="imgprod1" HorizontalAlignment="Left" Height="110" Margin="409,194,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Image x:Name="imgprod2" HorizontalAlignment="Left" Height="110" Margin="555,194,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Label x:Name="lblprod1" Content="Label" HorizontalAlignment="Left" Margin="409,304,0,0" VerticalAlignment="Top" Width="122"/>
        <Label x:Name="lblprod2" Content="Label" HorizontalAlignment="Left" Margin="555,304,0,0" VerticalAlignment="Top" Width="122"/>
        <Label x:Name="lblprod3" Content="Label" HorizontalAlignment="Left" Margin="702,304,0,0" VerticalAlignment="Top" Width="122"/>
        <Button x:Name="nextbtn" Content="" HorizontalAlignment="Left" Margin="702,520,0,0" VerticalAlignment="Top" Width="35" Height="35" BorderThickness="0" Click="nextbtn_Click">
            <Button.Background>
                <ImageBrush ImageSource="next-button.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="prevButton" Content="" HorizontalAlignment="Left" Margin="642,520,0,0" VerticalAlignment="Top" Width="35" Height="35" BorderThickness="0" Click="prevButton_Click">
            <Button.Background>
                <ImageBrush ImageSource="prev-button.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="findbtn" Content="Find" HorizontalAlignment="Left" Margin="904,153,0,0" VerticalAlignment="Top" Width="35" Height="23" Click="findbtn_Click" RenderTransformOrigin="-0.314,0.696"/>
        <Label x:Name="lblprod8" Content="Label" HorizontalAlignment="Left" Margin="849,469,0,0" VerticalAlignment="Top" Width="122"/>
        <Image x:Name="imgprod8" HorizontalAlignment="Left" Height="110" Margin="849,359,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Image x:Name="imgprod7" HorizontalAlignment="Left" Height="110" Margin="702,359,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Image x:Name="imgprod5" HorizontalAlignment="Left" Height="110" Margin="409,359,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Image x:Name="imgprod6" HorizontalAlignment="Left" Height="110" Margin="555,359,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Label x:Name="lblprod5" Content="Label" HorizontalAlignment="Left" Margin="409,469,0,0" VerticalAlignment="Top" Width="122"/>
        <Label x:Name="lblprod6" Content="Label" HorizontalAlignment="Left" Margin="555,469,0,0" VerticalAlignment="Top" Width="122"/>
        <Label x:Name="lblprod7" Content="Label" HorizontalAlignment="Left" Margin="702,469,0,0" VerticalAlignment="Top" Width="122"/>
        <Label Content="Search by Category:" HorizontalAlignment="Left" Margin="408,127,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="filtercb" HorizontalAlignment="Left" Margin="579,153,0,0" VerticalAlignment="Top" Width="134" Height="23">
            <ComboBox.BindingGroup>
                <BindingGroup/>
            </ComboBox.BindingGroup>
        </ComboBox>
        <Label Content="Filter:" HorizontalAlignment="Left" Margin="579,127,0,0" VerticalAlignment="Top"/>
        <Label Content="Keyword:" HorizontalAlignment="Left" Margin="731,127,0,0" VerticalAlignment="Top"/>
        <Image x:Name="imgprod1_Copy" HorizontalAlignment="Left" Height="110" Margin="36,176,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Image x:Name="imgprod1_Copy1" HorizontalAlignment="Left" Height="110" Margin="36,304,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>
        <Border BorderBrush="Black" BorderThickness="4 " HorizontalAlignment="Left" Height="483" Margin="252,105,0,0" VerticalAlignment="Top" Width="0"/>
        <Image x:Name="imgprod1_Copy2" HorizontalAlignment="Left" Height="110" Margin="36,432,0,0" VerticalAlignment="Top" Width="122" Source="dummy-img.png" Stretch="Fill"/>





    </Grid>
